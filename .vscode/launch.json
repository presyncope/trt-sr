{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "cwd": "${workspaceFolder}",
            "args": [
                "--input",
                "sample-5s.mp4",
                "--output",
                "output_xlsr.mp4",
                "--mode",
                "3",
                "--onnx-file",
                "models/xlsr-float.onnx/model_f16.onnx",
                "--plan-file",
                "models/xlsr-float.onnx/model_f16.plan",
                "--app-bin",
                "bin/real-esrgan-app",
            ],
            "console": "integratedTerminal"
        },
    ]
}