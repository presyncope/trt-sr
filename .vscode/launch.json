{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) superres c++",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/cpp-build/bin/superres_app",
            "args": ["--config", "scripts/cpp-build/config.ini"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "gdb에 자동 서식 지정 사용",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "디스어셈블리 버전을 Intel(으)로 설정",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "cwd": "${workspaceFolder}",
            "args": [],
            "console": "integratedTerminal"
        },
        {
            "name": "superres",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/superres.py",
            "cwd": "${workspaceFolder}",
            "args": [
                "--input",
                "sample-5s.mp4",
                "--output",
                "output.mp4",
                "--mode",
                "3",
                "--onnx-file",
                "models/realesrgan_general_x4v3_f16.onnx",
                "--plan-file",
                "model.plan",
                "--app-bin",
                "cpp/build/superres/superres_app",
                "--prescale",
                "0.5",
            ],
            "console": "integratedTerminal"
        },
    ]
}